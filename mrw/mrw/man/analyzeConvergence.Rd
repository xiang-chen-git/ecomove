\name{analyzeConvergence}
\alias{analyzeConvergence}
\title{Analyze convergence of JAGS fit}
\usage{
  analyzeConvergence(jagsResults, plotTraces = FALSE,
    plotCumu = FALSE)
}
\arguments{
  \item{jagsResults}{An \code{\link[=rjags-class]{rjags}}
  result}

  \item{plotTraces}{\code{TRUE} to plot traces of parmater
  chains, \code{FALSE} to skip plotting}

  \item{plotCumu}{\code{TRUE} to plot cumulative quanitiles
  of parmater chains, \code{FALSE} to skip plotting}
}
\description{
  Reports parameter means and standard deviations, as well
  as several convergence statistics: effective parameter
  size, autocorrelation at lag 1, Gelman and Rubin's
  diagnostic, and Geweke's Z.
}
\author{
  Chloe Bracis
}

